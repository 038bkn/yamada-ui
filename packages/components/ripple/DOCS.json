{
  "Ripple": {
    "onClear": { "type": "(key: Key) => void", "required": true },
    "ripples": { "type": "RippleOptions[]", "required": true },
    "as": {
      "type": "\"symbol\" | \"object\" | \"clipPath\" | \"filter\" | \"mask\" | \"marker\" | \"text\" | \"g\" | \"p\" | \"var\" | \"big\" | \"link\" | \"small\" | \"sub\" | \"sup\" | \"style\" | \"animate\" | \"span\" | \"a\" | \"abbr\" | ... 148 more ... | \"textPath\"",
      "required": false
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, disable ripple effects when pressing a element."
    }
  },
  "UseRipple": {
    "disabled": { "type": "boolean", "required": false },
    "isDisabled": { "type": "boolean", "required": false },
    "onPointerDown": { "type": "PointerEventHandler<T>", "required": false }
  }
}
